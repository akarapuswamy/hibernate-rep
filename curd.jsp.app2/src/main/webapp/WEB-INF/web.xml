<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>StudentsaveController</servlet-name>
  	<display-name>StudentsaveController</display-name>
  	<description></description>
  	<servlet-class>controller.StudentsaveController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteController</servlet-name>
  	<display-name>DeleteController</display-name>
  	<description></description>
  	<servlet-class>controller.DeleteController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentUpdateController</servlet-name>
  	<display-name>StudentUpdateController</display-name>
  	<description></description>
  	<servlet-class>controller.StudentUpdateController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentFindAllController</servlet-name>
  	<display-name>StudentFindAllController</display-name>
  	<description></description>
  	<servlet-class>controller.StudentFindAllController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FindAllController</servlet-name>
  	<display-name>FindAllController</display-name>
  	<description></description>
  	<servlet-class>controller.FindAllController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DelectController</servlet-name>
  	<display-name>DelectController</display-name>
  	<description></description>
  	<servlet-class>controller.DelectController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StudentsaveController</servlet-name>
  	<url-pattern>/StsC</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteController</servlet-name>
  	<url-pattern>/DeleteController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentUpdateController</servlet-name>
  	<url-pattern>/StudentUpdateController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentFindAllController</servlet-name>
  	<url-pattern>/StudentFindAllController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FindAllController</servlet-name>
  	<url-pattern>/findall</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DelectController</servlet-name>
  	<url-pattern>/DelectController</url-pattern>
  </servlet-mapping>
</web-app>
