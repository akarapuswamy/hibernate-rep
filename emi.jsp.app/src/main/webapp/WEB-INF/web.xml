<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Delectcontroller</servlet-name>
  	<display-name>Delectcontroller</display-name>
  	<description></description>
  	<servlet-class>controller.Delectcontroller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentSave</servlet-name>
  	<display-name>StudentSave</display-name>
  	<description></description>
  	<servlet-class>model.StudentSave</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FindAll</servlet-name>
  	<display-name>list</display-name>
  	<description></description>
  	<servlet-class>model.FindAll</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Updatecontroller</servlet-name>
  	<display-name>Updatecontroller</display-name>
  	<description></description>
  	<servlet-class>model.Updatecontroller</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Delectcontroller</servlet-name>
  	<url-pattern>/Delectcontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentSave</servlet-name>
  	<url-pattern>/StudentSave</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FindAll</servlet-name>
  	<url-pattern>/FindAll</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Updatecontroller</servlet-name>
  	<url-pattern>/Updatecontroller</url-pattern>
  </servlet-mapping>
</web-app>
